########################################################
## 
##  Configuration file for a new project
##
########################################################

## New project id (1 word, must be unique)
## Getting it from the env variable $AMET_PROJECT
run_id <- Sys.getenv("AMET_PROJECT")

## AQ model (e.g CMAQ)
model <- "CMAQ"

## login to db (e.g. username)
login <- Sys.getenv("AMET_LOGIN") 

## users email 
email <- "appel.wyat@epa.gov"

## Project description
description <- "AQ example project"

### Set flags.  These flags are used to either update an existing project
### Update a project will allow one to update the various fields in a existing project will retaining all existing data in the database.
### Remaking a project will allow one to update the various fields in an existing project and delete all existing data in the database.

check_missing <- "y"	# Flag to check table for missing columns (Must be set to T for new projects. Can be set to F for existing projects. Speeds up data loading to set to F)

update_table <- "n"	# Flag to update AMET project (y/n)

### Set remake flags (must set both to 'y').  WARNING: This will delete all data in the existing AMET data table and create a new empty data table. All data will be lost. ###
remake_table  <- "n"
remake_table2 <- "n"

### Set delete AMET table flags (must set both 'y').  WARNING: This will delete all data in the existing AMET table and remove the table from the database.  ###
delete_table  <- "n"
delete_table2 <- "n"
####################################

####################### DO NOT MODIFY #########################
## exit status if project exists and you decide not to overwrite
## 1 - error, so top level csh script stops
## 0 - no error, calling script will not stop
exit_exists <- 1

